{
  "DEV_MODE": false,
  "LOG_LEVEL": "info",
  "TENANT_CFG": {
    "tenant": {
      "auth": {
        "jwt_algorithm": "RS256",
        "jwt_audience": "urn:hyok-wrapper",
        "jwt_subject": "tenant-subject",
        "jwt_validation_certs": {
          "jwt-kid": "config/tenant-jwt-kid.pubkey"
        }
      },
      "backend": {
        "jwe-kid": {
          "key_consumer_cert": "config/tenant_servicename_key_consumer.crt",
          "vault_path": "vault-path:latest"
        }
      }
    }
  },
  "VAULT_JWT_DEFAULT_ROLE": "hyok",
  "VAULT_URL": "http://vault:8200"
}
